{
  "name": "xml2js-xpath",
  "version": "0.9.1",
  "description": "Search xml2js JSON documents with XPath query strings.",
  "keywords": [
    "xml2js",
    "xpath",
    "search",
    "parse"
  ],
  "homepage": "https://github.com/dsummersl/node-xml2js-xpath",
  "bugs": {
    "url": "https://github.com/dsummersl/node-xml2js-xpath/issues"
  },
  "license": "ISC",
  "author": "Dane Summers",
  "main": "xpath.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dsummersl/node-xml2js-xpath.git"
  },
  "scripts": {
    "prepublish": "./node_modules/.bin/babel src/xpath.js > xpath.js",
    "test": "nyc mocha src/**/*.test.js"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "@babel/runtime": "^7.4.3",
    "@istanbuljs/nyc-config-babel": "^2.1.0",
    "babel-plugin-istanbul": "^5.1.3",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.0.0",
    "xml2js": "^0.4.19"
  },
  "typings": "xml2js-xpath",
  "babel": {
    "plugins": [
      "@babel/plugin-transform-runtime"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "extends": "@istanbul/nyc-config-babel"
  }
}
